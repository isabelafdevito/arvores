#include<iostream>
using namespace std;

// Verifica se n é primo
bool primo(int n)
{
	bool p = true; // true se n for primo, false caso contrário
	int i; // contador

	if (n == 1)
		return false;
	else
		for(i = 2; i < n; i++)
			if(n % i == 0)
				p = false;

	return p;
}

int main()
{
    float *vet; // ponteiro para o vetor
    float *p; // ponteiro aux para varrer o vetor
    int i; // contador
    int N; // tamanho do vetor
    int cont = 0; // conta os numeros primos
    
    // Lendo tamanho do vetor
    cin >> N;
    
    // Alocando memória para o vetor
    vet = new float [N];
    
    // Lendo os elementos do vetor
    for(i=0;i<N;i++)
        cin >> vet[i];
        
    // Procurando elemento no vetor
    p = vet;
    for(i=0;i<N;i++)
    {
        if(primo(*p) == true)
             cont++;
        p++;     
    }   
    
    // Apresentando resultado
    cout << cont << " numeros primos" << endl;
    
    // Liberando memória ocupada pelo vetor
    delete [] vet;
    
    return 0;
}
